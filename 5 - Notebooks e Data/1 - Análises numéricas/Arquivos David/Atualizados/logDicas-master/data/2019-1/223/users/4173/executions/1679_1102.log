	elif (h <= H):
		cal = (pi/3) * (H-h)**2 * (3 * r - H+h)
		cil = pi * r **2 * (H-2*r)
		V = esf + cil - cal
		V *= 1000
		V = round(V,3)	
		print("Entradas:", H,",",h,",",r)
		print("Volume: ", V, "litros")

else:
	print("Entradas:", H,",",h,",",r)
