print("estafilococo:", round(X[0], 1))
print("salmonela:", round(X[1], 1))
print("coli:", round(X[2], 1))

# Verificando qual bactéria tem a menor população

if X[0] == min(X):
	print("estafilococo")
if X[1] == min(X):
	print("salmonela")
if X[2] == min(X):
	print("coli")
